    <ul class="horizontal slashes">
      <li class="twc-first">
        <a analyticsname="" href="/">
          Editorial Framework &amp; Style Guide
        </a>
      </li>
      {{#if page.section }}
      <li>
        <a analyticsname="" href="/toolkit">
          {{ section }}
        </a>
      </li>
      {{/if}}
      {{#if page.subsection }}
      <li>
        <a analyticsname="" href="/primary-elements">
          {{ subsection }}
        </a>
      </li>
      {{/if}}
      {{#if page.zoom }}
